{
  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "strict": true,
    "esModuleInterop": true,
    "declarationDir": "./types"
  },
  "include" :[
    "./*.js",
    "./datasource/**/*.js"
  ],
  "exclude": [
    "test/**/*",
    "node_modules/**/*",
    "types/**/*",
    "index.js"
  ]
}